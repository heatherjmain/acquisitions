{
  "name": "acquisitions",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc -p ./tsconfig.json",
    "clean:code": "pnpm run lint:fix && pnpm run format",
    "eslint": "npx eslint",
    "format": "prettier --write \"**/*.{ts,js,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,js,json,md}\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "start:api": "pnpm run start:localdb && wait-on tcp:localhost:5432 && npx tsx --watch src/graphql.local.ts",
    "start:localdb": "docker rm -f acquisitions-postgres || true && docker run --name acquisitions-postgres -e POSTGRES_USER=admin -e POSTGRES_PASSWORD=secretPassword -e POSTGRES_DB=postgres -p 5432:5432 -d postgres:15",
    "test": "jest --passWithNoTests --coverage --no-cache --ci",
    "test:update": "jest --updateSnapshot --passWithNoTests --ci"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
